{
    "global": {
        "files": "/src/**/*.*",
        "skipGeneratedGatewayClasses": true,
        "skipGeneratedPersistentClasses": true,
        "skipGeneratedFunctionGroups": true
    },
    "syntax": {
        "version": "v702",
        "errorNamespace": "^(Z|Y)",
        "globalConstants": [],
        "globalMacros": []
    },
    "dependencies": [
        {
            "url": "https://github.com/FreHu/deps",
            "files": "/src/**/*.*"
        }
    ],
    "rules": {
        "downport": true,
        "check_syntax": true,
        "parser_missing_space": true
    }
}